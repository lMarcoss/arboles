digraph ArbolBinario{
node [shape = record,height=.1];
nodo0[label = "<f0> |<f1> 1| <f2> "];
nodo1[label = "<f0> |<f1> 5| <f2> "];
nodo2[label = "<f0> |<f1> 6| <f2> "];
nodo3[label = "<f0> |<f1> 8| <f2> "];
nodo4[label = "<f0> |<f1> 7| <f2> "];
"nodo0":f2 -> "nodo1":f1;
"nodo1":f2 -> "nodo2":f1;
"nodo2":f2 -> "nodo3":f1;
"nodo3":f0 -> "nodo4":f1;

}